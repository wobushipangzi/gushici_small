/* pages/library/library-list/library-list.wxss */

.toptitle {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid #666;
}

.cont-list {
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #e5e5e5;
}

.poetry-title {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0;
}

.poetry-title .timu {
  font-weight: bold;
}
.poetry-cont{
  display: flex;
  flex-wrap: nowrap;
}
